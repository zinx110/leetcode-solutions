{
    "description": "Test cases for 104. Maximum Depth of Binary Tree",
    "tests": [
        {
            "input": {
                "root": [3, 9, 20, null, null, 15, 7]
            },
            "expectedOutput": 3
        },
        {
            "input": {
                "root": [1, null, 2]
            },
            "expectedOutput": 2
        },
        {
            "input": {
                "root": []
            },
            "expectedOutput": 0
        },
        {
            "input": {
                "root": [1]
            },
            "expectedOutput": 1
        }
    ],
    "constraints": {
        "numberOfNodes": {"min": 0, "max": 10000},
        "nodeValue": {"min": -100, "max": 100}
    }
}

