{
    "description": "Test cases for 226. Invert Binary Tree",
    "tests": [
        {
            "input": {
                "root": [4, 2, 7, 1, 3, 6, 9]
            },
            "expectedOutput": [4, 7, 2, 9, 6, 3, 1]
        },
        {
            "input": {
                "root": [2, 1, 3]
            },
            "expectedOutput": [2, 3, 1]
        },
        {
            "input": {
                "root": []
            },
            "expectedOutput": []
        },
        {
            "input": {
                "root": [1]
            },
            "expectedOutput": [1]
        }
    ],
    "constraints": {
        "numberOfNodes": {"min": 0, "max": 100},
        "nodeValue": {"min": -100, "max": 100}
    }
}

